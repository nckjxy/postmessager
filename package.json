{
    "name": "postmessager",
    "version": "1.0.10",
    "description": "postmessage集成方案",
    "main": "lib/index.js",
    "unpkg": "lib/index.js",
    "module": "lib/index.esm.js",
    "files": [
        "lib"
    ],
    "directories": {
        "lib": "lib",
        "src": "src"
    },
    "scripts": {
        "build": "rm -rf ./lib && rollup -c",
        "build:umd": "webpack --config webpack.config.js",
        "dev": "rollup -c -w",
        "test": "node test/test.js",
        "pretest": "npm run build",
        "docs": "jsdoc2md src/**/*.js > API.md",
        "prettier": "prettier --write \"**/*.{js,ts,json,md}\"",
        "pub": "git add . && git commit -m $npm_package_version && git pull && git push"
    },
    "dependencies": {
        "core-js": "^3.16.1",
        "js-cool": "^1.1.0"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@babel/core": "^7.15.0",
        "@babel/eslint-parser": "^7.15.0",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "@rollup/plugin-babel": "^5.2.3",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-node-resolve": "^13.0.2",
        "@saqqdy/prettier-config": "^1.0.3",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "@typescript-eslint/parser": "^4.29.1",
        "babel-loader": "^8.2.2",
        "core-decorators": "^0.20.0",
        "esbuild": "^0.12.20",
        "eslint": "^7.31.0",
        "eslint-config-sets": "^1.0.10",
        "jsdoc-to-markdown": "^7.0.1",
        "prettier": "^2.2.1",
        "progress-bar-webpack-plugin": "^2.1.0",
        "rollup": "^2.56.2",
        "rollup-plugin-esbuild": "^4.5.0",
        "rollup-plugin-typescript2": "^0.30.0",
        "rollup-plugin-uglify": "^6.0.4",
        "typescript": "^4.3.5",
        "webpack": "^5.50.0",
        "webpack-cli": "^4.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/postmessager.git"
    },
    "keywords": [
        "postmessager",
        "saqqdy"
    ],
    "author": {
        "name": "saqqdy",
