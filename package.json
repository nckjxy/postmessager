{
    "name": "postmessager",
    "version": "1.0.5",
    "description": "postmessage集成方案",
    "main": "lib/index.js",
    "unpkg": "lib/index.js",
    "module": "lib/index.esm.js",
    "files": [
        "lib"
    ],
    "directories": {
        "lib": "lib",
        "src": "src"
    },
    "scripts": {
        "build": "rm -rf ./lib && rollup -c",
        "dev": "rollup -c -w",
        "test": "node test/test.js",
        "pretest": "npm run build",
        "docs": "jsdoc2md src/**/*.js > API.md",
        "prettier": "prettier --write \"**/*.{js,ts,json,md}\"",
        "pub": "git add . && git commit -m $npm_package_version && git pull && git push"
    },
    "dependencies": {
        "core-js": "^3.15.2",
        "js-cool": "^1.0.9"
    },
    "devDependencies": {
        "@babel/cli": "^7.12.13",
        "@babel/core": "^7.12.13",
        "@babel/eslint-parser": "^7.12.16",
        "@babel/plugin-transform-runtime": "^7.12.15",
        "@babel/preset-env": "^7.12.13",
        "@rollup/plugin-babel": "^5.2.3",
        "@rollup/plugin-commonjs": "^19.0.1",
        "@rollup/plugin-node-resolve": "^13.0.2",
        "@saqqdy/prettier-config": "^1.0.3",
        "babel-loader": "^8.2.2",
        "esbuild": "^0.12.15",
        "eslint": "^7.31.0",
        "eslint-config-sets": "^1.0.10",
        "jsdoc-to-markdown": "^6.0.1",
        "prettier": "^2.2.1",
        "rollup": "^2.38.5",
        "rollup-plugin-esbuild": "^4.5.0",
        "rollup-plugin-terser": "^7.0.2"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/postmessager.git"
    },
    "keywords": [
        "postmessager",
        "saqqdy"
    ],
    "author": {
        "name": "saqqdy",
