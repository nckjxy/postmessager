{
    "name": "postmessager",
    "version": "1.0.10",
    "description": "postmessage集成方案",
    "main": "lib/index.js",
    "unpkg": "lib/index.js",
    "module": "lib/index.esm.js",
    "files": [
        "lib",
        "typings"
    ],
    "directories": {
        "lib": "lib",
        "src": "src"
    },
    "scripts": {
        "build": "rm -rf ./lib && rollup -c",
        "build:umd": "webpack --config webpack.config.js",
        "build:types": "run-s build-temp-types roll-types",
        "build-temp-types": "tsc --emitDeclarationOnly --outDir temp/ -p src/",
        "patch-types": "node script/patchTypes",
        "roll-types": "api-extractor run && rimraf temp",
        "dev": "rollup -c -w",
        "dist": "run-s eslint prettier build build:umd build:types docs prettier:docs",
        "test": "node test/test.js",
        "pretest": "npm run build",
        "docs": "jsdoc2md src/**/*.js > API.md",
        "prettier": "prettier --write \"**/*.{js,ts,json,md}\"",
        "prettier:docs": "prettier --write \"*.{md}\"",
        "pub": "git add . && git commit -m $npm_package_version && git pull && git push"
    },
    "dependencies": {
        "core-js": "^3.16.1",
        "js-cool": "^2.0.1"
    },
    "devDependencies": {
        "@babel/cli": "^7.14.8",
        "@babel/core": "^7.15.0",
        "@babel/eslint-parser": "^7.15.0",
        "@babel/plugin-transform-runtime": "^7.15.0",
        "@babel/preset-env": "^7.15.0",
        "@babel/preset-typescript": "^7.15.0",
        "@microsoft/api-extractor": "^7.18.6",
        "@rollup/plugin-babel": "^5.2.3",
        "@rollup/plugin-commonjs": "^20.0.0",
        "@rollup/plugin-node-resolve": "^13.0.2",
        "@saqqdy/prettier-config": "^1.0.3",
        "@typescript-eslint/eslint-plugin": "^4.29.1",
        "@typescript-eslint/parser": "^4.29.1",
        "babel-loader": "^8.2.2",
        "core-decorators": "^0.20.0",
        "esbuild": "^0.12.20",
        "eslint": "^7.31.0",
        "eslint-config-sets": "^1.0.10",
        "jsdoc-to-markdown": "^7.0.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1",
        "progress-bar-webpack-plugin": "^2.1.0",
        "rimraf": "^3.0.2",
        "rollup": "^2.56.2",
        "rollup-plugin-typescript2": "^0.30.0",
        "rollup-plugin-uglify": "^6.0.4",
        "typescript": "^4.3.5",
        "webpack": "^5.50.0",
        "webpack-cli": "^4.8.0",
        "webpack-node-externals": "^3.0.0"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/saqqdy/postmessager.git"
    },
    "keywords": [
        "postmessager",
        "saqqdy"
    ],
    "author": {
        "name": "saqqdy",
        "email": "saqqdy@qq.com",
        "url": "http://www.saqqdy.com"
    },
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/saqqdy/postmessager/issues"
    },
    "homepage": "https://github.com/saqqdy/postmessager#readme"
}
